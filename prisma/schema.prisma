// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id             Int    @id @default(autoincrement())
    nome           String
    dataNascimento String
    email          String @unique
    senha          String
    cpf            String
    rua            String
    numeroRua      String
    bairro         String
    cidade         String
    cep            String
    numeroTel      String
    numeroCel      String
}
